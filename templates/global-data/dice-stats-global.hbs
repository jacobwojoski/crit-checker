                <td>{{localize "DICE_STATS_TEXT.both_forms.table_values.total_rolls"}}</th>
                <td>{{TOTAL_ROLLS.[0]}}</th>
            </tr>
            <tr>
                <td>{{localize "DICE_STATS_TEXT.both_forms.table_values.mean"}}</th>
                <td>{{MEAN.[0]}}</th>
            </tr>
            <tr>
                <td>{{localize "DICE_STATS_TEXT.both_forms.table_values.median"}}</th>
